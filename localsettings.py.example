## Local Settings for the osim-rl-grader

CROWDAI_URL="API endpoint for the CrowdAI Instance, ex : http://www.crowdai.org/api/external_graders/"
CROWDAI_CHALLENGE_ID="Challenge ID of the corresponding challenge"
CROWDAI_CHALLENGE_CLIENT_NAME="nips-2017-learning-to-run"
CROWDAI_TOKEN="Authentication Token provided by CrowdAI"

CROWDAI_REPLAY_DATA_VERSION=0

REDIS_HOST="127.0.0.1"
REDIS_PORT=6379
DISPLAY=":100"

S3_ACCESS_KEY="ACCESS_ID_FOR_AWS_USER"
S3_SECRET_KEY="SECRET_KEY_FOR_AWS_USER"
S3_BUCKET="Bucket Name where the GIFs will be uploaded to"

DEBUG_MODE=False

SEED_MAP = [
  <<EXAMPLE_SEED_1>>,
  <<EXAMPLE_SEED_2>>,
  <<EXAMPLE_SEED_3>>,
]

RENDER_LOGO=0

SUBMISSION_WINDOW_TTL = 86400 # 24 hours * 60 minutes * 60 seconds
MAX_SUBMISSIONS_PER_WINDOW = 5

ENV_TTL = 20*60 # 20 minutes * 60 seconds
MAX_PARALLEL_ENVS = 3
